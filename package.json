{
  "name": "react-native-multiitems-align",
  "author": "syzh",
  "version": "0.1.0",
  "description": "this component is to help users to align array of object conveniently.",
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "babel-preset-flow": "^6.23.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.76.0",
    "husky": "^0.14.3",
    "jest-expo": "~27.0.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "react-dom": "^16.4.1",
    "react-native-scripts": "1.14.0",
    "react-test-renderer": "16.3.1"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test:unit": "jest",
    "pretty": "prettier --semi false --print-width 100 --single-quote--trailing-comma all --write \"src/**/*.js\"",
    "precommit": "lint-staged && npm run test",
    "flow": "flow",
    "lint": "node node_modules/eslint/bin/eslint src/**/*.js",
    "lint:root": "node node_modules/eslint/bin/eslint App.js",
    "flow-stop": "flow stop",
    "test": "npm run lint && npm run flow && jest"
  },
  "lint-staged": {
    "*.js": [
      "npm run pretty",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/?(*.)test.js?(x)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ]
  },
  "dependencies": {
    "expo": "^27.0.1",
    "react": "16.3.1",
    "react-native": "~0.55.2"
  }
}
